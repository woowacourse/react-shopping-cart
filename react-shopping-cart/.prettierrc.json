{
  "singleQuote": true,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "printWidth": 100,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "importOrder": [
    "^(@|pages)(.*|$)",
    "^(@|components/@shared)(.*|$)",
    "^(@|components)(.*|$)",
    "^(@|redux/)(.*|$)",
    "^(@|hooks)(.*|$)",
    "^(@|styles)(.*|$)",
    "^(@|(assets|constants|utils|api))(.*|$)"
  ],
  "importOrderSeparation": true
}
